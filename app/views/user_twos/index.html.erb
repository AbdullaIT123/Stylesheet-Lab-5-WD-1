<p id="notice"><%= notice %></p>

<h1>User Twos</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_twos.each do |user_two| %>
      <tr>
        <td><%= user_two.name %></td>
        <td><%= user_two.email %></td>
        <td><%= link_to 'Show', user_two %></td>
        <td><%= link_to 'Edit', edit_user_two_path(user_two) %></td>
        <td><%= link_to 'Destroy', user_two, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Two', new_user_two_path %>
